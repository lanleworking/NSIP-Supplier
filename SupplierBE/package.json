{
    "name": "SupplierBE",
    "version": "1.0.50",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "bun run --watch src/index.ts",
        "start": "bun run src/index.ts",
        "env": "cp .env.example .env && echo 'Environment variables copied from .env.example to .env'",
        "build": "bun build  --compile  --minify-whitespace  --minify-syntax  --target bun --outfile server  ./src/index.ts",
        "build:dist": "bun build  --minify-whitespace  --minify-syntax --outfile ./dist/index.js   ./src/index.ts"
    },
    "dependencies": {
        "@elysiajs/cors": "^1.3.3",
        "@elysiajs/jwt": "^1.3.2",
        "@elysiajs/static": "^1.3.0",
        "@types/lodash": "^4.17.20",
        "@types/nodemailer": "^6.4.17",
        "dayjs": "^1.11.13",
        "elysia": "latest",
        "lodash": "^4.17.21",
        "mssql": "^11.0.1",
        "nodemailer": "^7.0.5",
        "typeorm": "^0.3.25"
    },
    "devDependencies": {
        "bun-types": "latest"
    },
    "module": "src/index.js"
}
